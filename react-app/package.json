    "@emotion/styled": "11.11.0",
    "@mdi/js": "7.4.47",
    "@mdi/react": "1.6.1",
    "@mui/icons-material": "5.15.6",
    "@mui/material": "^5.15.6",
    "@mui/x-data-grid": "^7.0.0",
    "@mui/x-date-pickers": "^7.0.0",
    "dayjs": "1.11.10",
    "node-xlsx": "0.23.0",
    "react": "18.2.0",
