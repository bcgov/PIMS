    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
