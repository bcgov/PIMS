    "react-click-away-listener": "2.2.3",
    "react-datepicker": "6.9.0",
    "react-dom": "18.3.1",
    "react-draggable": "4.4.5",
    "react-error-boundary": "4.0.11",
    "react-icons": "5.2.1",
    "react-leaflet": "4.2.1",
    "react-paginate": "8.2.0",
    "react-redux": "8.1.2",
