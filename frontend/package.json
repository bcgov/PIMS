    "crypto-js": "4.2.0",
    "dequal": "2.0.3",
    "formik": "2.4.3",
    "history": "5.3.0",
    "keycloak-js": "24.0.1",
    "leaflet": "1.9.4",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "papaparse": "5.4.1",
    "polylabel": "1.1.0",
    "react": "18.3.1",
    "react-app-polyfill": "3.0.0",
    "react-bootstrap": "2.10.0",
