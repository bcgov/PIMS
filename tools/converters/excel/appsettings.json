{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serialization": {
    "Json": {
      "IgnoreNullValues": true,
      "PropertyNameCaseInsensitive": true,
      "PropertyNamingPolicy": "CamelCase",
      "WriteIndented": true
    }
  },
  "Converter": {
    "Sources": [
      {
        "Name": "SIS",
        "File": "./Data/properties.xlsx",
        "Output": "./Data/properties.json",
        "SheetName": "Properties_Main_Query",
        "Columns": {
          "Status": {
            "Name": "Status",
            "Type": "String"
          },
          "Fiscal Year": {
            "Name": "FiscalYear",
            "Type": "Int32"
          },
          "Agency": {
            "Name": "AgencyCode",
            "Type": "String",
            "ValueSwitch": [
              {
                "Search": "AVED",
                "Value": "AEST"
              },
              {
                "Search": "MTICS",
                "Value": "CITZ"
              },
              {
                "Search": "CSNR",
                "Value": "EMPR"
              }
            ]
          },
          "Sub Agency": {
            "Name": "SubAgency",
            "Type": "String"
          },
          "Property Type": {
            "Name": "PropertyType",
            "Type": "String"
          },
          "Parcel ID": {
            "Name": "PID",
            "Type": "String"
          },
          "Local ID": {
            "Name": "LocalId",
            "Type": "String"
          },
          "Description": {
            "Name": "Description",
            "Type": "String"
          },
          "Assessed Value": {
            "Name": "Assessed",
            "Type": "Decimal"
          },
          "Classification": {
            "Name": "Classification",
            "Type": "String"
          },
          "Civic Address": {
            "Name": "CivicAddress",
            "Type": "String"
          },
          "City": {
            "Name": "City",
            "Type": "String"
          },
          "Postal Code": {
            "Name": "Postal",
            "Type": "String"
          },
          "Latitude": {
            "Name": "Latitude",
            "Type": "Double",
            "Converter": "GeoLocationConverter",
            "ConverterArgs": ["CivicAddress", "City"],
            "OutputTo": ["Latitude", "Longitude"],
            "Skip": "AlreadySet"
          },
          "Longitude": {
            "Name": "Longitude",
            "Type": "Double",
            "Skip": "AlreadySet"
          },
          "Land Area": {
            "Name": "LandArea",
            "Type": "Single"
          },
          "Land Legal Description": {
            "Name": "LandLegalDescription",
            "Type": "String"
          },
          "Building Type of Construction": {
            "Name": "BuildingConstructionType",
            "Type": "String"
          },
          "Building Floors": {
            "Name": "BuildingFloorCount",
            "Type": "Int32"
          },
          "Building Predominant Use": {
            "Name": "BuildingPredominateUse",
            "Type": "String"
          },
          "Building Rentable Area": {
            "Name": "RentableArea",
            "Type": "Single"
          },
          "Building Tenancy": {
            "Name": "BuildingTenancy",
            "Type": "String",
            "Converter": "ConvertToTenancy"
          },
          "Building Net Book Value": {
            "Name": "NetBook",
            "Type": "Decimal"
          }
        }
      },
      {
        "Name": "Todds",
        "File": "./Data/properties-todds.xlsx",
        "Output": "./Data/properties-todds.json",
        "SheetName": "PIMS_point_Data_AllMay2109",
