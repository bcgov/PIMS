          },
          "BuildingPredominateUse": {
            "Name": "BuildingPredominateUse",
            "Type": "String"
          },
          "Building_R": {
            "Name": "RentableArea",
            "Type": "Single"
          },
          "BuildingTenancy": {
            "Name": "BuildingTenancy",
            "Type": "String",
            "Converter": "ConvertToTenancy"
          },
          "Building_N": {
            "Name": "NetBook",
            "Type": "Decimal"
          }
        }
      },
      {
        "Name": "Projects",
        "File": "./Data/projects.xlsx",
        "Output": "./Data/projects.json",
        "SheetName": "ReportingExport",
        "Columns": {
          "RAEG#": {
            "Name": "ProjectNumber",
            "Type": "String"
          },
          "FY": {
            "Name": "ActualFiscalYear",
            "Type": "Int32",
            "Converter": "ConvertToFiscalYear"
          },
          "Major_Activity": {
            "Name": "Activity",
            "Type": "String"
          },
          "Sales_Status": {
            "Name": "Status",
            "Type": "String",
            "ValueSwitch": [
              {
                "Search": "(?i)Pre-Marketing",
                "Value": "SPL-PM"
              },
              {
                "Search": "(?i)Marketing",
                "Value": "SPL-M"
              },
              {
                "Search": "(?i)Unconditionally Sold",
