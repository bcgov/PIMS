          },
          "LocalId": {
            "Name": "LocalId",
            "Type": "String"
          },
          "Description": {
            "Name": "Description",
            "Type": "String"
          },
          "AssessedValue": {
            "Name": "Assessed",
            "Type": "Decimal"
          },
          "Classification": {
            "Name": "Classification",
            "Type": "String"
          },
          "CivicAddress": {
            "Name": "CivicAddress",
            "Type": "String"
          },
          "City": {
            "Name": "City",
            "Type": "String"
          },
          "Postal": {
            "Name": "Postal",
            "Type": "String"
          },
          "Latitude": {
            "Name": "Latitude",
            "Type": "Double",
            "Converter": "GeoLocationConverter",
            "ConverterArgs": ["CivicAddress", "City"],
            "OutputTo": ["Latitude", "Longitude"],
            "Skip": "AlreadySet"
          },
          "Longitude": {
            "Name": "Longitude",
            "Type": "Double",
            "Skip": "AlreadySet"
