{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serialization": {
    "Json": {
      "IgnoreNullValues": true,
      "PropertyNameCaseInsensitive": true,
      "PropertyNamingPolicy": "CamelCase",
      "WriteIndented": true
    }
  },
  "Converter": {
    "Sources": [
      {
        "File": "./Data/projects.xlsx",
        "Output": "./Data/projects.json",
        "SheetName": "ReportingExport",
        "Columns": {
          "RAEG#": {
            "Name": "ProjectNumber",
            "Type": "String"
          },
          "FY": {
            "Name": "ActualFiscalYear",
            "Type": "Int32",
            "Converter": "ConvertToFiscalYear"
          },
          "Major_Activity": {
            "Name": "Activity",
            "Type": "String"
          },
          "Sales_Status": {
            "Name": "Status",
            "Type": "String",
            "ValueSwitch": [
              {
                "Search": "(?i)Pre-Marketing",
                "Value": "SPL-PM"
              },
              {
                "Search": "(?i)Marketing",
                "Value": "SPL-M"
              },
              {
                "Search": "(?i)Unconditionally Sold",
                "Value": "SPL-CIP"
              },
              {
                "Search": "(?i)Conditionally Sold",
                "Value": "SPL-CIP"
              },
              {
                "Search": "(?i)Complete",
                "Value": "DIS"
              },
              {
                "Search": "(?i)Complete - Adjustment to prior year sale",
                "Value": "DIS"
              },
              {
                "Search": ".*",
                "Value": "AP-SPL"
              }
            ]
          },
          "Sector": {
            "Name": "Agency",
            "Type": "String",
            "ValueSwitch": [
              {
                "Search": "(?i)TRAN-PAVCO",
                "Value": "TRAN"
              },
              {
                "Search": "(?i)CSCD",
                "Value": "CITZ"
              },
              {
                "Search": "(?i)E&M",
                "Value": "CITZ"
              },
              {
                "Search": "(?i)JAG",
                "Value": "CITZ"
              }
            ]
          },
          "Description": {
            "Name": "Description",
            "Type": "String"
          },
          "CMV": {
            "Name": "Estimated",
            "Type": "Decimal"
          },
          "NBV": {
            "Name": "NetBook",
            "Type": "Decimal"
          },
          "Sales_Costs": {
            "Name": "SalesCost",
            "Type": "Decimal"
          },
          "Net_Proceeds": {
            "Name": "NetProceeds",
            "Type": "Decimal"
          },
          "Baseline_Integrity_Check": {
            "Name": "Variance",
            "Type": "Decimal?"
          },
          "Status": {
            "Name": "Risk",
            "Type": "String"
          },
          "Marketing_Start": {
            "Name": "MarketedOn",
            "Type": "DateTime?"
          },
          "Comp_Date": {
            "Name": "CompletedOn",
            "Type": "DateTime?",
            "Converter": "ConvertToDate",
            "ValueSwitch": [
              {
                "Search": "N/A",
                "Value": {}
              },
              {
                "Search": "6/23/21013",
                "Value": "6/23/2013"
              }
            ]
          },
          "Comment_Weekly_Review": {
            "Name": "PrivateNote",
            "Type": "String"
          },
          "Item Type": {
            "Name": "ItemType",
            "Type": "String"
          },
          "Path": {
            "Name": "Path",
            "Type": "String"
          },
          "Weekly_Integrity_Check": {
            "Name": "PriorNetProceeds",
            "Type": "Decimal?"
          },
          "Program_Cost": {
            "Name": "ProgramCost",
            "Type": "Decimal"
          },
          "Gain_(Loss)": {
            "Name": "GainLoss",
            "Type": "Decimal"
          },
          "OCG_Fin_Stmts": {
            "Name": "OcgFinancialStatement",
            "Type": "Decimal?"
          },
          "Interest_Component": {
            "Name": "InterestComponent",
            "Type": "Decimal"
          },
          "Rpt_FY": {
            "Name": "ReportedFiscalYear",
            "Type": "Int32",
            "Converter": "ConvertToFiscalYear"
          },
          "Manager": {
            "Name": "Manager",
            "Type": "String"
          },
          "SLIP": {
            "Name": "SaleWithLeaseInPlace",
            "Type": "Boolean"
          },
          "Financial_Notes": {
            "Name": "FinancialNote",
            "Type": "String"
          },
          "Interest from Enhanced Referral": {
            "Name": "AgencyResponseNote",
            "Type": "String"
          },
          "Date of Interest Rcvd": {
            "Name": "AgencyResponseDate",
            "Type": "DateTime?",
            "Converter": "ConvertToDate"
          }
        }
      }
    ]
  }
}
