          "Status": {
            "Name": "Risk",
            "Type": "String"
          },
          "Marketing_Start": {
            "Name": "MarketedOn",
            "Type": "DateTime?"
          },
          "Comp_Date": {
            "Name": "CompletedOn",
            "Type": "DateTime?",
            "Converter": "ConvertToDate",
            "ValueSwitch": [
              {
                "Search": "N/A",
                "Value": {}
              },
              {
                "Search": "6/23/21013",
